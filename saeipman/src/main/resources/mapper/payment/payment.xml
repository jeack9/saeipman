<mapper namespace="com.saeipman.app.payment.mapper.PaymentMapper">

	<!-- 관리비 기간조회 -->
	<select id="selectMonth" resultType="PaymentVO">
		SELECT *
		FROM month_gwanlibi
		WHERE TO_CHAR(payment_month,'yyyymm') = #{payMonth}
	</select>
